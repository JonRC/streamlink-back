{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": ["standard", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    // "indent": ["error", "tab"]
    "no-redeclare": "off",
    "no-use-before-define": "off"
  }
}
